{
  "name": "@catalyft/core",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.js"
    },
    "./dist/api/aria-program": {
      "types": "./dist/api/aria-program.d.ts",
      "import": "./dist/api/aria-program.js",
      "require": "./dist/api/aria-program.js"
    },
    "./dist/api/insights": {
      "types": "./dist/api/insights.d.ts",
      "import": "./dist/api/insights.js",
      "require": "./dist/api/insights.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"]
  }
}