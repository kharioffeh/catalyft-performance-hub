{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "postinstall": "node scripts/fix-podspec.js",
    "start": "expo start",
    "start:staging": "NODE_ENV=staging expo start",
    "start:production": "NODE_ENV=production expo start",
    "android": "expo run:android",
    "android:staging": "NODE_ENV=staging expo start --android",
    "android:production": "NODE_ENV=production expo start --android",
    "ios": "expo run:ios",
    "ios:staging": "NODE_ENV=staging expo start --ios",
    "ios:production": "NODE_ENV=production expo start --ios",
    "web": "expo start --web",
    "build:staging": "NODE_ENV=staging expo build",
    "build:production": "./scripts/build-production.sh --all",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "build:e2e:ios": "detox build --configuration ios.sim.debug",
    "build:e2e:android": "detox build --configuration android.emu.debug",
    "detox:validate": "node scripts/validate-e2e-setup.js",
    "detox:build:ios": "detox build --configuration ios.sim.debug",
    "detox:test:ios": "detox test --configuration ios.sim.debug --artifacts-location artifacts --max-workers 1",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:test:android": "detox test --configuration android.emu.debug --artifacts-location artifacts --max-workers 1",
    "detox:smoke:ios": "detox test --configuration ios.sim.debug e2e/smoke.e2e.ts",
    "detox:smoke:android": "detox test --configuration android.emu.debug e2e/smoke.e2e.ts",
    "fix:android-build": "node scripts/fix-android-build.js",
    "env:print": "node -v && npm -v && java -version || true",
    "android:clean-prebuild": "rm -rf android && CI=1 npx expo prebuild --platform android --clean",
    "android:run": "npx expo run:android",
    "android:validate": "npm run android:clean-prebuild && npm run android:run",
    "ios:clean-prebuild": "rm -rf ios && CI=1 npx expo prebuild --platform ios --clean",
    "ios:pod": "cd ios && npx pod-install",
    "ios:run": "npx expo run:ios",
    "ios:validate": "npm run ios:clean-prebuild && npm run ios:pod && npm run ios:run",
    "ios:verify-build": "node scripts/verify-ios-build.js",
    "validate:native": "npm run android:validate",
    "e2e:smoke": "npm run detox:test:android && npm run detox:test:ios",
    "build:android:production": "./scripts/build-android-production.sh",
    "build:android:ci": "GRADLE_OPTS='-Xmx12288m -XX:MaxMetaspaceSize=3072m -XX:+UseG1GC' ./scripts/build-android-production.sh",
    "build:android:memory-optimized": "./scripts/build-android-memory-optimized.sh",
    "build:android:ultra-minimal": "./scripts/build-android-ultra-minimal.sh",
    "build:android:essential-only": "./scripts/build-android-essential-only.sh",
    "build:android:fallback": "./scripts/build-android-fallback.sh",
    "build:android:sequential": "./scripts/build-android-sequential.sh",
    "build:android:minimal-expo": "./scripts/build-android-minimal-expo.sh",
    "build:android:pure-rn": "./scripts/build-android-pure-rn.sh",
    "build:android:kotlin-fix": "./scripts/build-android-kotlin-fix.sh",
    "build:android:no-kotlin": "./scripts/build-android-no-kotlin.sh",
    "build:android:debug": "./scripts/build-android-debug.sh",
    "build:android:absolute-minimum": "./scripts/build-android-absolute-minimum.sh",
    "build:android:bypass-expo": "./scripts/build-android-bypass-expo.sh",
    "build:android:no-native": "./scripts/build-android-no-native.sh",
    "build:android:extreme-minimum": "./scripts/build-android-extreme-minimum.sh",
    "build:android:no-expo-modules": "./scripts/build-android-no-expo-modules.sh",
    "build:android:pure-js": "./scripts/build-android-pure-js.sh",
    "build:android:ultra-extreme": "./scripts/build-android-ultra-extreme.sh",
    "build:android:minimal-only": "./scripts/build-android-minimal-only.sh",
    "build:android:no-native-at-all": "./scripts/build-android-no-native-at-all.sh",
    "build:android:no-problematic-modules": "./scripts/build-android-no-problematic-modules.sh",
    "build:android:no-native-modules": "./scripts/build-android-no-native-modules.sh",
    "build:android:ultimate-minimum": "./scripts/build-android-ultimate-minimum.sh",
    "build:android:no-gesture-handler": "./scripts/build-android-no-gesture-handler.sh",
    "build:android:final-minimum": "./scripts/build-android-final-minimum.sh",
    "build:android:no-safe-area-context": "./scripts/build-android-no-safe-area-context.sh",
    "build:android:no-native-modules-at-all": "./scripts/build-android-no-native-modules-at-all.sh",
    "build:android:absolute-final": "./scripts/build-android-absolute-final.sh",
    "build:android:no-linear-gradient": "./scripts/build-android-no-linear-gradient.sh",
    "build:android:no-expo-modules-at-all": "./scripts/build-android-no-expo-modules-at-all.sh",
    "build:android:kotlin-reflection-fix": "./scripts/build-android-kotlin-reflection-fix.sh",
    "build:android:no-kotlin-compilation": "./scripts/build-android-no-kotlin-compilation.sh",
    "build:android:debug-kotlin-fix": "./scripts/build-android-debug-kotlin-fix.sh",
    "build:android:pure-react-native": "./scripts/build-android-pure-react-native.sh",
    "build:ios:production": "./scripts/build-ios-production.sh",
    "test:ios-features": "./scripts/test-ios-features.sh",
    "setup:multi-user-testing": "./scripts/setup-multi-user-testing.sh",
    "deploy:ios:app-store": "./scripts/deploy-ios-app-store.sh",
    "deploy:android:play-store": "./scripts/deploy-android-play-store.sh",
    "test:ios:free": "./scripts/test-ios-free.sh",
    "start:expo-go": "./scripts/start-expo-go.sh",
    "build:all:production": "./scripts/build-production.sh --all",
    "test:social": "node src/scripts/runSocialTests.js",
    "test:social:unit": "jest --testPathPattern=social --coverage",
    "test:social:integration": "jest --testPathPattern=social.integration --coverage",
    "test:api-connections": "node src/scripts/test-api-connections.js",
    "test:wearable-integration": "jest --testPathPattern=wearable --coverage",
    "test:real-time-features": "jest --testPathPattern=realtime --coverage"
  },
