# Analytics Service Keys
SEGMENT_WRITE_KEY=your_segment_write_key
MIXPANEL_TOKEN=your_mixpanel_project_token
AMPLITUDE_API_KEY=your_amplitude_api_key

# Firebase Configuration
# Get these from Firebase Console → Project Settings → Add Web App
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_APP_ID=1:123456789012:web:abcdef123456
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# App Store IDs for Rating Prompt
IOS_APP_STORE_ID=your_ios_app_store_id
ANDROID_PLAY_STORE_ID=com.catalyft.mobile

# Backend API Endpoint
API_BASE_URL=https://api.yourfitnessapp.com

# Supabase Configuration (You already have these)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Crashlytics
CRASHLYTICS_ENABLED=true
CRASHLYTICS_DEBUG=false

# Feature Flags
FEATURE_FLAGS_API_KEY=your_feature_flags_key
FEATURE_FLAGS_ENVIRONMENT=production

# Environment
NODE_ENV=production
APP_ENV=production
DEBUG_MODE=false